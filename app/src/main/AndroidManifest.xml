<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ggec.uitest">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity android:name=".ui.MainActivity"
            android:theme="@android:style/Theme.Holo.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.ETActivity"
            android:theme="@android:style/Theme.Holo.NoActionBar"/>

        <activity android:name=".ui.LVActivity"
            android:theme="@android:style/Theme.Holo.NoActionBar"/>

        <activity android:name=".ui.UIActivity"
            android:theme="@android:style/Theme.Holo.NoActionBar"/>

        <activity android:name=".ui.FragmentDialogActivity"
            android:theme="@android:style/Theme.Holo.NoActionBar"/>

        <activity android:name=".ui.SocketActivity"
            android:theme="@android:style/Theme.Holo.NoActionBar"/>

        <activity android:name=".ui.ChineseCodeActivity"
            android:theme="@android:style/Theme.Holo.NoActionBar"/>

        <activity android:name=".ui.OneLifeActivity"
            android:theme="@android:style/Theme.Holo.NoActionBar"/>

        <activity android:name=".ui.TwoLifeActivity"
            android:theme="@android:style/Theme.Holo.NoActionBar"/>

    </application>

</manifest>